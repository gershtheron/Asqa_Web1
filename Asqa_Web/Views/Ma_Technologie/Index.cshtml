@{
}
@model IEnumerable<Asqa_Web.Models.Entities.Ma_Technologie>

@{
    ViewData["Title"] = "Ma_Technologie List";
}

<h2>@ViewData["Title"]</h2>

<p>
    <a asp-action="Create" class="btn btn-primary">Create New</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>Mitarbeiter Name</th>
            <th>Technologie Name</th>
            <th>Kompetenz Name</th>
            <th>Seit Jahr</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Mitarbeiter.Ma_Nachname</td>
                <td>@item.Technologie.Tech_name</td>
                <td>@item.Kompetenz.Komp_name</td>
                <td>@item.SeitJahr.ToString("yyyy")</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>